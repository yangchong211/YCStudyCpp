<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!--个人中心资料左边文字抽取-->
    <style name="MeInfoTextLeftStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/blackText4</item>
    </style>

    <style name="MeInfoTextRightStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:drawableRight">@drawable/ic_back_right</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/blackText2</item>
    </style>

</resources>